{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/price-box",
  "version": "0.1.0",
  "title": "Price Box",
  "category": "widgets",
  "icon": "dashicons-money-alt",
  "description": "A Gutenberg block to show pricing box.",
  "attributes": {
    "subTitle": {
      "type": "string",
      "source": "text",
      "selector": "p"
    },
    "price": {
      "type": "string",
      "source": "text",
      "selector": "h1"
    },
    "points": {
      "type": "string",
      "source": "html",
      "selector": "ul"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "price-box",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
